  <div class="panel panel-default">
  <div class="panel-heading"><i class="fa fa-edit"></i>&nbsp;New Sql</div>
  <div class="panel-body">
    <form class="form-horizontal" accept-charset="UTF-8" action="/qs/create" method="post" role="form">
      <div class="form-group">
        <label for="name" class="control-label col-lg-2">description</label>
        <div class="col-lg-8">
          <input type="text" id="qs_name" name="qs[name]"  class="form-control"/>
        </div>
      </div> 
      <div class="form-group">
        <label for="sql" class="control-label col-lg-2">Sql string</label>
        <div class="col-lg-8">
          <textarea type="text" id="qs_sqlstring" name="qs[sqlstring]"  class="form-control" rows="3">
          </textarea>
        </div>
      </div> 
      <div class="form-group">
        <label for="status" class="control-label col-lg-2">State</label>
        <div class="col-lg-8">
          <input type="text" id="qs_state" name="qs[state]"  class="form-control"/>
        </div>
      </div>      
      <div class="form-group">
        <label for="title" class="control-label col-lg-2"></label>
        <div class="col-lg-8">
          <input class="btn btn-default btn-info" name="commit" type="submit" value="Add" />
        </div>
      </div>          
    </form>
  </div> 
</div>



<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Sql</div>

  <!-- Table -->
  <table class="table">
  	<tr>
		<th>#</th>
		<th>Description</th>
		<th>Sql</th>
		<th>Status</th>
		<th>CreatedAt</th>
		<th>UpdatedAt</th>
  	</tr>
    <% _.each(qlist, function (item) { %>
	    <tr>
		    <td><%= item.id %></td>
		    <td><%= item.name %></td>
		    <td><%= item.sqlstring %></td>
		    <td><%= item.state %></td>
		    <td><%= item.createdAt %></td>
		    <td><%= item.updatedAt %></td>
	    </tr>
    <% }) %>
  </table>
</div>





